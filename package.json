{
    "name": "foundecms",
    "displayName": "founder cms freemarker",
    "description": "方正翔宇VS Code插件 Freemarker版",
    "icon": "images/icon.png",
    "version": "0.1.25",
    "publisher": "Triangel",
    "license":"See License section in README.md",
    "homepage":"https://github.com/triangeljs/founderCmsLabelFreemarker/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/triangeljs/founderCmsLabelFreemarker.git"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type":"object",
            "title":"founderCMS",
            "properties": {
                "founderCMS.loginInfo": {
                    "type": "array",
                    "default": [{
                        "Title": "添加站点登录信息。",
                        "url": "http://127.0.0.1/",
                        "UserCode": "用户名",
                        "UserPassword": "密码",
                        "DocLibID": "市场方正设置为19，内部方正设置为24"
                    }],
                    "description": "填写登录地址、用户名和密码"
                }
            }
        },
        "commands": [
            {
                "command": "founder.login",
                "title": "方正登录",
                "category": "ciic"
            },
            {
                "command": "founder.addTag",
                "title": "添加方正标签",
                "category": "ciic"
            },
            {
                "command": "founder.editTag",
                "title": "编辑方正标签",
                "category": "ciic"
            },
            {
                "command": "founder.columnQuery",
                "title": "方正栏目信息查询",
                "category": "ciic"
            },
            {
                "command": "founder.newsQuery",
                "title": "方正文章信息查询",
                "category": "ciic"
            },
            {
                "command": "founder.templateDownload",
                "title": "方正模板下载",
                "category": "ciic"
            },
            {
                "command": "founder.templateUpload",
                "title": "方正模板上传",
                "category": "ciic"
            },
            {
                "command": "founder.columnMake",
                "title": "方正栏目生成",
                "category": "ciic"
            },
            {
                "command": "founder.articlelistAttr",
                "title": "插入稿件列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.articlelistpageAttr",
                "title": "插入分页稿件列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.articleAttr",
                "title": "插入稿件内容属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.columnlistAttr",
                "title": "插入栏目列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.advcolumnlistAttr",
                "title": "插入高级栏目列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.positionAttr",
                "title": "插入当前位置属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.blockarticlelist",
                "title": "插入手动区块稿件属性",
                "category": "ciic_Type"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "resourceLangId == true",
                    "command": "founder.login",
                    "group": "ciic@1"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.addTag",
                    "group": "ciic@2"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.editTag",
                    "group": "ciic@3"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.columnQuery",
                    "group": "ciic@4"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.newsQuery",
                    "group": "ciic@5"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.templateDownload",
                    "group": "ciic@6"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.templateUpload",
                    "group": "ciic@7"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.columnMake",
                    "group": "ciic@8"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.articlelistAttr",
                    "group": "ciic_Type@1"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.articlelistpageAttr",
                    "group": "ciic_Type@2"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.articleAttr",
                    "group": "ciic_Type@3"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.columnlistAttr",
                    "group": "ciic_Type@4"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.advcolumnlistAttr",
                    "group": "ciic_Type@5"
                },
                {
                    "when": "resourceLangId == true",
                    "command": "founder.positionAttr",
                    "group": "ciic_Type@6"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.blockarticlelist",
                    "group": "ciic_Type@7"
                }
            ]
        },
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/ciic.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "cheerio": "^1.0.0-rc.1",
        "request":"^2.81.0",
        "moment":"^2.18.1"
    },
    "dependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "cheerio": "^1.0.0-rc.1",
        "request":"^2.81.0",
        "moment":"^2.18.1"
    }
}