{
    "name": "foundecms",
    "displayName": "founder cms freemarker",
    "description": "方正翔宇VS Code插件 Freemarker版",
    "version": "0.1.10",
    "publisher": "Triangel",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type":"object",
            "title":"founderCMS",
            "properties": {
                "founderCMS.loginInfo": {
                    "type": "array",
                    "default": [{
                        "Title": "添加站点登录信息。",
                        "url": "http://127.0.0.1/",
                        "UserCode": "用户名",
                        "UserPassword": "密码"
                    }],
                    "description": "填写登录地址、用户名和密码"
                }
            }
        },
        "commands": [
            {
                "command": "founder.login",
                "title": "Ⅰ 方正登录",
                "category": "ciic"
            },
            {
                "command": "founder.addTag",
                "title": "Ⅱ 添加方正标签",
                "category": "ciic"
            },
            {
                "command": "founder.editTag",
                "title": "Ⅲ 编辑方正标签",
                "category": "ciic"
            },
            {
                "command": "founder.columnQuery",
                "title": "Ⅳ 方正栏目信息查询",
                "category": "ciic"
            },
            {
                "command": "founder.newsQuery",
                "title": "Ⅴ 方正文章信息查询",
                "category": "ciic"
            },
            {
                "command": "founder.templateDownload",
                "title": "Ⅵ 方正模板下载",
                "category": "ciic"
            },
            {
                "command": "founder.articlelistAttr",
                "title": "插入稿件列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.articlelistpageAttr",
                "title": "插入分页稿件列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.articleAttr",
                "title": "插入稿件内容属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.columnlistAttr",
                "title": "插入栏目列表属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.positionAttr",
                "title": "插入当前位置属性",
                "category": "ciic_Type"
            },
            {
                "command": "founder.blockarticlelist",
                "title": "插入手动区块稿件属性",
                "category": "ciic_Type"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "resourceLangId == html",
                    "command": "founder.login",
                    "group": "ciic"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.addTag",
                    "group": "ciic"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.editTag",
                    "group": "ciic"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.columnQuery",
                    "group": "ciic"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.newsQuery",
                    "group": "ciic"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.templateDownload",
                    "group": "ciic"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.articlelistAttr",
                    "group": "ciic_Type"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.articlelistpageAttr",
                    "group": "ciic_Type"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.articleAttr",
                    "group": "ciic_Type"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.columnlistAttr",
                    "group": "ciic_Type"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.positionAttr",
                    "group": "ciic_Type"
                },
                {
                    "when": "resourceLangId == html",
                    "command": "founder.blockarticlelist",
                    "group": "ciic_Type"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "cheerio": "^1.0.0-rc.1",
        "request":"^2.81.0",
        "moment":"^2.18.1"
    },
    "dependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "cheerio": "^1.0.0-rc.1",
        "request":"^2.81.0",
        "moment":"^2.18.1"
    }
}